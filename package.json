{
  "name": "risevest-test",
  "version": "1.0.0",
  "description": "Risevest Test",
  "main": "src/main.ts",
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "prod": "npx prisma migrate deploy && npx prisma db seed && node dist/src/main",
    "start": "NODE_ENV=production node dist/src/main.js",
    "dev": "stale-dep && nodemon",
    "format": "prettier --write --cache \"./**/*.{js,ts,md}\"",
    "format:check": "prettier --check \"./**/*.{js,ts,md}\"",
    "lint": "eslint --cache .",
    "lint:fix": "eslint . --cache --fix",
    "build": "tsc -p .",
    "typecheck": "tsc --noEmit",
    "db:deploy": "npx prisma migrate deploy",
    "db:migrate": "npx prisma migrate dev",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "db:generate-schemas": "prisma generate && prisma format",
    "postinstall": "stale-dep -u && simple-git-hooks",
    "test": "vitest run",
    "test:unit": "vitest run -c vitest.unit.config.ts",
    "test:watch:unit": "vitest -c vitest.unit.config.ts",
    "test:watch": "vitest --watch",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:coverage:unit": "vitest run -c vitest.unit.config.ts --coverage",
    "test:e2e": "vitest run -c vitest.e2e.config.ts",
    "test:e2e:watch": "vitest -c vitest.e2e.config.ts",
    "docker:compose": "docker compose up -d",
    "docker:build": "docker build -t risevest-api . --progress=plain --no-cache",
    "docker:run": "docker run -d -p 7000:7000 risevest-api"
  },
  "author": "Damilare Anjorin",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.2.0",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.9.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "redis": "^4.6.8",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.10.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@damilaredev/eslint-config": "^0.0.19",
    "@damilaredev/tsconfig": "^1.5.2",
    "@prisma/generator-helper": "^5.2.0",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.4",
    "@types/supertest": "^2.0.12",
    "@vitest/coverage-v8": "^0.34.2",
    "@vitest/ui": "^0.34.2",
    "errorhandler": "^1.5.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "pnpm": "^8.6.12",
    "prettier": "^3.0.2",
    "prisma": "^5.2.0",
    "simple-git-hooks": "^2.9.0",
    "stale-dep": "^0.7.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vite-aliases": "^0.11.2",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.2",
    "zod-prisma": "^0.5.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
